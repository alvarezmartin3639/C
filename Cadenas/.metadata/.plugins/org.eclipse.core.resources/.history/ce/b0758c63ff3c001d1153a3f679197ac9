#include <stdio.h>
#include <stdlib.h>
#include "utnValidacion.h"

int main(void) {
	setbuf(stdout,NULL);
	char alfanumerico[24] = "ab555gT6";
	char telefono[24] = "4232-2222";

	if(esAlfanumerico(alfanumerico) == 1)
		printf("\nEs alfanumerico!");
	else
		printf("\nNo es alfanumerico!");

	if(esNumeroTelefonico(telefono) == 1)
		printf("\nEs telefono!");
	else
		printf("\nEs telefono!");

	return EXIT_SUCCESS;
}
