#ifndef GENERIC_H_
#define GENERIC_H_

#include <stdio.h>

typedef struct{

	int *pNumero = 0;
	float *pNumeroFlotante;
	char *pTexto;
	void (*pRestart)(void);

}eGeneric;

int genericRestart(eGeneric *generic);

#endif /* GENERIC_H_ */
