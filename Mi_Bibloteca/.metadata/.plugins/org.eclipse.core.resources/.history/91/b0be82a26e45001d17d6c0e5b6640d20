#ifndef GENERIC_H_
#define GENERIC_H_

#include <stdio.h>

typedef struct {

	int *pNumero ;
	float *pNumeroFlotante = NULL;
	char *pTexto = NULL;
	void (*pRestart)(void) = NULL;

} eGeneric;

int genericRestart(eGeneric *generic);

#endif /* GENERIC_H_ */
