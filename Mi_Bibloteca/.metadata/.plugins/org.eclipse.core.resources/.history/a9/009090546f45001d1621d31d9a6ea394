#include <stdio.h>

#ifndef GENERIC_H_
#define GENERIC_H_


typedef struct {

	int *pNumero;
	float *pNumeroFlotante = 0;
	char *pTexto = NULL;
	void (*pRestart)(void) = NULL;

} eGeneric;

int genericRestart(eGeneric *generic);

#endif /* GENERIC_H_ */
