#include <stdio.h>
#include <stdlib.h>
#include "utnArray.h"

int main(void) {
	setbuf(stdout,NULL);

	//utnArrays.h
	int enteros[5];
	float flotantes[5];
	char caracteres[5];
	char string[5][30];

	printf("\nProbando Entero");
	incializarInt(enteros, 5, 0);
	for(int i = 0; i<5 ; i++){
		if(enteros[i] != 0)
			printf("\nFracaso");
	}

	return EXIT_SUCCESS;
}
