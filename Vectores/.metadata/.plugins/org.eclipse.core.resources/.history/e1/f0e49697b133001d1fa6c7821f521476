#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout,NULL);
	int numeroInput[5];

	for(int i =0 ;i<5;i++){
		printf("\nIngrese un numero: ");
		fflush(stdin);
		scanf("%d", &numeroInput[i]);
	}



	return EXIT_SUCCESS;
}
